{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "World Tree",
  "type": "object",
  "properties": {
    "eden_vitality_index": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 100.0
    },
    "status": {
      "type": "string",
      "enum": [
        "Radiant (Eden Flourishes)",
        "Stable (Eden is Sustained)",
        "Wilting (Eden Weakens)",
        "Fading (Eden in Peril)",
        "Dying (Eden in Collapse)"
      ]
    },
    "user_count": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": ["eden_vitality_index", "status", "user_count"]
}
