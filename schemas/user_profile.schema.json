{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User Profile Schema",
  "type": "object",
  "properties": {
    "mbti": {
      "type": "string",
      "description": "16-type Myers-Briggs personality code"
    },
    "iq": {
      "type": "integer",
      "description": "Abstract reasoning score (Raven's Matrices preferred)"
    },
    "eq": {
      "type": "integer",
      "description": "Emotional intelligence quotient"
    },
    "moral": {
      "type": "string",
      "description": "Dominant moral foundation (e.g. care, liberty)"
    },
    "disclosure": {
      "$ref": "./disclosure_payload.schema.json"
    }
  },
  "required": ["mbti", "iq", "eq", "moral"],
  "additionalProperties": false
}
